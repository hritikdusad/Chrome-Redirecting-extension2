{
	"name": "Video redirects",
	"description": "It redirects to a google page when user opens a page containing videos",
	"version":"1.0",
	"manifest_version":2,

	"icons":{
		"128":"icon128.png",
		"48": "icon48.png",
		"16":"icon16.png"
	},

	"content_scripts":[
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],

	"permissions" : [
					"tabs",
					"<all_urls>"

				] 		

	
}
